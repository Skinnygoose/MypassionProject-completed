﻿@model VirtualStore.Models.Vaporizer


@{
    ViewBag.Title = "View";
}

<a class="btn btn-outline-dark" href="/Vaporizer/List">Go back to list</a>
<a class="btn btn-outline-dark" href="/Vaporizer/Delete/@Model.vaporizerId">Delete</a>
<a class="btn btn-outline-dark" href="/Vaporizer/Edit/@Model.vaporizerId">Update</a>
<h2>@Model.VaporizerName</h2>
<div>Flavour: @Model.FlavourName</div>
<div>Customer Ratings: @Model.CustomerRatings</div>
<div>Profit(in percentage): @Model.Profit</div>
